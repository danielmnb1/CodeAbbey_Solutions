using System;

class Program
{
    static void Main()
    {
        int N = int.Parse(Console.ReadLine());

        string[] valores = Console.ReadLine().Split();

        int sumaTotal = 0;

        for (int i = 0; i < N; i++)
        {
            sumaTotal += int.Parse(valores[i]);
        }

        Console.WriteLine(sumaTotal);
    }
}
